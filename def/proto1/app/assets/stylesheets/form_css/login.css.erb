#main_form {
  background: url(<%= asset_path "phr-main-container-login.png" %>) 0 0 no-repeat;
  border: 0 !important;
  margin: 5px auto;
  padding-top: 0px !important;
  padding-bottom: 0px !important;
  padding-top: 8px !important;
  text-align: center;  
  width: 925px;
}

#main_form #fe_splash_grp_0
{
	background:url(<%= asset_path "phr-feature-children-computer.jpg" %>) no-repeat scroll 14px 0 transparent;
	left:-31px;
