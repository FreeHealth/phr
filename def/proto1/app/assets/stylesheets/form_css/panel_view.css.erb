div.panel_timeline {
  background-color: #D9E6F0;
  margin: 15px 15px 0 15px;
  padding: 0;
  white-space: nowrap;
}
div.panel_timeline td.fixed_col {
  width:400px; /* 330px, check box +18 */
}

div.panel_timeline table{
  /* very import to have this to make vertical scrollbar on the 2nd table to
     appear correctly
  */
  table-layout: fixed;
  border: 1px solid #ccc;
}
div.panel_timeline td {
/*  background: #f5f5f5; */
  padding: 0px;
  vertical-align:top;
}

div.panel_timeline {
  background-color: #f5f5f5;
}

div.panel_def {
  position:relative;
  width:100%;  /* 400px, check box +18 */
  height:auto;
  top:0px;
}

div.panel_def table {
/*  background-color: #d8ebfc; */
  border-collapse: separate; 
  border-spacing: 0;
  padding:  0px;
  border-color: #CCCCCC;
  border: 0;
  width:100%;  /* 400px, check box +18 */
}

div.panel_def th {
  background: none repeat scroll 0 0 #E9E9E9;
  padding: 1px 4px 1px 0;
  text-align: left;
}

div.panel_def .test_name {
  width: 100%; /* 240px; */
}

div.panel_def .sparkline_chart {
  width: 88px; /* check box +18 */
}

div.sparkline {
  cursor: pointer;
}

div.panel_def input[type='checkbox'] {
  float: left;
  margin:0px;
  height:16px;
}
