div.panel_timeline_grp {
  background-color: #D9E6F0;
  border: 0;
  margin: 0 0 30px;
  padding: 5px 0 15px 0;
}
div.test_data_table_wrapper {
  height:auto;
  width:100%;
  border-width:2px;
}
div.test_data_table {
  overflow:auto;
  height:auto;
  width:100%;
}
div.test_data_table table {
  /* do not use collapse so that we can control the special borders added
  for grouped columns */
  border-collapse: separate; /*collapse; */
  border-spacing: 0;
  empty-cells:show;
  padding: 0;
  table-layout: fixed;
  text-align: left;
  white-space: nowrap;
  border-top: 0;
  border-bottom: 0;
  border-left: 0;
}
div.test_data_table table td {
  border-left: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
  font-size: 13px !important;
  padding: 6px;  
} 
div.test_data_table th {
  background: none repeat scroll 0 0 #E9E9E9; 
  border-left: 1px solid #CCCCCC;
  font-size: 13px !important;
  padding-left: 5px;
  padding-right: 5px;
  text-align: center;
  white-space: nowrap;
  width: 5em; /*60px;*/
  padding-top: 6px;
  padding-bottom: 6px;
  
}

div.test_data_table tr:first-child th {
  padding-top: 12px;
  padding-bottom: 0;
}
div.test_data_table tr:nth-child(3) th{
  padding-bottom: 11px;
  padding-top: 0;
  border-bottom: 1px solid #CCCCCC;	
}
div.test_data_table td.no_data {
  text-align:center;
  white-space:nowrap;
}

div.test_data_table input.flag.readonly_field {
  color: #B12121;
  text-align: right;
  width:1.5em;
  margin-right:2px;
}

